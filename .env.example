# Token opsional untuk mengakses endpoint instalasi
INSTALL_API_KEY=

# Konfigurasi koneksi database master/root (digunakan untuk validasi prasyarat)
INSTALL_DB_HOST=localhost
INSTALL_DB_PORT=3306
INSTALL_DB_ROOT_USER=root
INSTALL_DB_ROOT_PASSWORD=

# Lokasi template aplikasi (opsional)
INSTALL_TEMPLATE_DIR=server/templates

# Versi minimal runtime
MIN_NODE_VERSION_MAJOR=18
MIN_PHP_VERSION=8.1

# Informasi versi PHP saat ini (placeholder)
CURRENT_PHP_VERSION=8.2.0
